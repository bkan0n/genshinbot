{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "url": {
        "type": "string",
        "format": "uri"
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "thumbnail": {
        "type": "string",
        "format": "uri"
      },
      "color": {
        "type": "string"
      }
    },
    "required": ["name", "url", "image", "thumbnail", "color"],
    "additionalProperties": false
  }
}
